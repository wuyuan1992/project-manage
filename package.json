{
  "name": "p1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && npx prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "format": "prettier --write --ignore-path .gitignore .",
    "prisma:seed": "ts-node prisma/seeders/seed.ts",
    "prisma:up": "dotenv -e .env.local -- npx prisma db push",
    "prisma:migrate": "dotenv -e .env.local -- npx prisma migrate dev --name $1",
    "prisma:reset": "dotenv -e .env.local -- npx prisma migrate reset --skip-seed"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@react-email/components": "^0.0.12",
    "@react-email/render": "^0.0.10",
    "aws-sdk": "^2.1510.0",
    "next": "14.0.3",
    "prisma": "^5.6.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "husky": "^8.0.3",
    "postcss": "^8",
    "prettier": "3.1.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  }
}
